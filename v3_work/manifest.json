{
    "manifest_version": 3,
    "name": "work",
    "version": "1.0",
    "description": "good for job",
    "author": "Liuyao China",
    "icons": {
        "16": "img/icon128.png",
        "32": "img/icon128.png",
        "48": "img/icon128.png",
        "128": "img/icon128.png"
    },

    "content_scripts": [
        {
            "run_at": "document_end",
            "js":["js/base.js", "js/inject.js"],
            "css": ["css/inject.css"],
            "matches": ["*://127.0.0.1/*", "*://*.21tb.com/*"],
            "exclude_matches": [
                "*://*/nacos/",
                "*://kibana.21tb.com/*",
                "*://gitlab.21tb.com/*",
                "*://mail.21tb.com/*",
                "*://yufaredis.21tb.com/redis/",
                "*://yufajenkins.21tb.com/*",
                "*://jenkins.21tb.com/*"
            ]
        },
        {
            "run_at": "document_end",
            "js":["js/openframe.js"],
            "all_frames": true,
            "matches": ["*://127.0.0.1/*", "*://*.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js":["js/base.js", "js/yufaredis.js"],
            "css": ["css/inject.css", "css/yufaredis.css"],
            "matches": ["*://yufaredis.21tb.com/redis/"]
        },
        {
            "run_at": "document_end",
            "js":["js/base.js", "js/jenkins.js"],
            "css": ["css/inject.css"],
            "matches": ["*://jenkins.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js":["js/base.js", "js/yufajenkins.js"],
            "css": ["css/inject.css"],
            "matches": ["*://yufajenkins.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js": ["js/base.js", "js/kibana.js"],
            "css": ["css/inject.css"],
            "matches": ["*://kibana.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js": ["js/base.js", "js/mail.js"],
            "css": ["css/inject.css"],
            "matches": ["*://mail.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js": ["js/base.js", "js/gitlab.js"],
            "css": ["css/inject.css"],
            "matches": ["*://gitlab.21tb.com/*"]
        },
        {
            "run_at": "document_end",
            "js": ["js/base.js", "js/nacos.js"],
            "css": ["css/inject.css", "css/nacos.css"],
            "matches": ["*://*/nacos/"]
        }
    ],

    "permissions": [
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "work.ffp@61linux.com",
            "strict_min_version": "91.1.0"
        }
    }
    
} 