{
  "manifest_version": 3,
  "name": "http",
  "version": "1.0",
  "description": "发送站内http请求",
  "icons": {
    "16": "img/http.png",
    "32": "img/http.png",
    "48": "img/http.png",
    "128": "img/http.png"
  },

  "content_scripts": [
    {
      "run_at": "document_end",
      "js":["inject.js"],
      "css": ["inject.css"],
      "matches":["<all_urls>"]
    }
  ],

  "permissions": [
    "webRequest"
  ],


  "browser_specific_settings": {
    "gecko": {
      "id": "v3_postman.ffp@61linux.com",
      "strict_min_version": "91.1.0"
    }
  }
}